
@{
    ViewBag.Title = "Projects";
}
<a class=" btn btn-primary" href="https://localhost:44389/Home/Index">Index</a>
<a class=" btn btn-dark" href="https://localhost:44389/Home/Contact">Contact <br /> Information</a>
<a class=" btn btn-danger" href="https://localhost:44389/Home/Education">Education</a>


<h2>@ViewBag.Message</h2>
<<table class="table-bordered table">


    <tr>
        <th>Course</th>
        <th>Projects</th>
        <th>Description</th>
    </tr>

    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Client</td>
            <td>@p.Email</td>
        </tr>
    }
    </tr>

</table>


